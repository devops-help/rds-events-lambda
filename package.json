{
  "name": "rds-events-lambda",
  "version": "1.0.0",
  "description": "RDS Event delivery to Slack via SNS and Lambda",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Greg Messner <greg@devops.help>",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "scripts": {
    "pretest": "eslint --format table --ignore-path .gitignore .",
    "lint": "eslint --format table --ignore-path .gitignore .",
    "sam:build": "sam build --manifest package.json",
    "sam:package": "cross-var sam package --s3-bucket $S3_BUCKET --output-template-file packaged.yaml --debug",
    "test": "mocha tests/unit/"
  },
  "nyc": {
    "check-coverage": true,
    "exclude": "tests/**",
    "reporter": [
      "text",
      "html"
    ],
    "branches": 80,
    "functions": 80,
    "lines": 90,
    "statements": 80
  },
  "eslintConfig": {
    "env": {
      "jasmine": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-mixed-spaces-and-tabs": 1,
      "semi": "error",
      "no-console": 0
    },
    "plugins": [
      "security",
      "jsdoc",
      "mocha"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "plugin:security/recommended"
    ]
  },
  "dependencies": {
    "lambda-log": "^2.3.1"
  },
  "devDependencies": {
    "aws-sdk": "^2.568.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-var-no-babel": "^1.2.0",
    "commander": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-jsdoc": "^7.2.3",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-security": "^1.4.0",
    "jsdoc": "^3.6.3",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1"
  }
}
